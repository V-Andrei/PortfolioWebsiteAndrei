@page
@model IndexModel
@{
    ViewData["Title"] = "Home page";
}

<!-- hero
    ================================================== -->
<section id="hero" class="s-hero target-section" data-parallax="scroll" data-image-src="images/hero-bg.jpg" data-natural-width=3000 data-natural-height=2000 data-position-y=center>

    <div class="row hero-content">

        <div class="column large-full">

            <h1>
                Hello World, <br>
                I'm Andrei Vracianu, <br>
                a frontend developer <br>
                & designer.
            </h1>

            <ul class="hero-social">
                <li>
                    <a href="https://www.facebook.com/andrei.vrancianu" title="">Facebook</a>
                </li>
                <li>
                    <a href="https://www.linkedin.com/in/andrei-vr%C3%A2ncianu" title="">Linkedin</a>
                </li>
                <li>
                    <a href="https://github.com/V-Andrei" title="">GitHub</a>
                </li>
            </ul> <!-- end hero-social -->

        </div>

    </div> <!-- end hero-content -->

    <div class="hero-scroll">
        <a href="#about" class="scroll-link smoothscroll">
            <svg xmlns="http://www.w3.org/2000/svg" width="24" height="24" viewBox="0 0 24 24"><path d="M12 24l-8-9h6v-15h4v15h6z" /></svg>
        </a>
    </div> <!-- end hero-scroll -->

</section> <!-- end s-hero -->
<!-- about
================================================== -->
<section id="about" class="s-about target-section">

    <div class="s-about__section s-about__section--profile">

        <div class="right-vert-line"></div>

        <div class="row">
            <div class="column large-6 medium-8 tab-full">

                <div class="section-intro" data-num="01" data-aos="fade-up">
                    <h3 class="subhead">About Me</h3>
                    <h1 class="display-1">I'm the kind of person who isn't afraid of challenges.</h1>
                </div>

                <div class="profile-pic" data-aos="fade-up">
                    <img src="images/profile-pic.jpg"
                         srcset="images/profile-pic.jpg 1x, images/profile-pic@2x.jpg 2x" alt="">
                </div>

                <h3 data-aos="fade-up">Profile</h3>

                <p data-aos="fade-up">
                    NET developer specialized in web and passionate about game development.
                    I have enrolled in Wantsome IT Academy to learn more about .NET web development and allowing me to expand my knowledge in programming.
                    Also, I am a self-taught logo/web designer.
                </p>

            </div>
        </div>

    </div> <!-- end s-about__section--profile -->

    <div class="s-about__section">

        <div class="row">
            <div class="column">
                <h3 data-aos="fade-up">Education</h3>
            </div>
        </div>

        <div class="row block-large-1-2 block-900-full work-positions">

            <div class="column" data-aos="fade-up">
                <div class="position">
                    <div class="position__header">
                        <h6>
                            <span class="position__co">Colegiul Economic "Ion Ghica" Bacau</span>
                            <span class="position__pos">High School Diploma</span>
                        </h6>
                        <div class="position__timeframe">
                            2011 - 2015
                        </div>
                    </div>

                    <p><b>General skills:</b> Romanian language, Mathematics, Business communication, English language, Information and communication technology.</p>
                    <p><b>Professional skills:</b> Simulated a real business environment.</p>
                </div>
            </div> <!-- end column -->
            <div class="column" data-aos="fade-up">
                <div class="position">
                    <div class="position__header">
                        <h6>
                            <span class="position__co">Wantsome The Friendly IT Academy Iasi</span>
                            <span class="position__pos">.NET Programming Course</span>
                        </h6>
                        <div class="position__timeframe">
                            2019 - 2020
                        </div>
                    </div>

                    <p><b>General skills:</b> Algorithms, OOP, SQL, HTML, CSS, Bootstrap, ASP.NET, ADO.NET.</p>
                    <p><b>Professional skills:</b> Website store made with ASP.NET, including Bootstrap and Java Script</p>
                    
                </div>
            </div> <!-- end column -->
            

        </div> <!-- work positions -->

    </div> <!-- end s-about__section -->
    <div class="s-about__section">
        <div class="row">
            <div class="column">
                <h3 data-aos="fade-up">Career</h3>
            </div>
        </div>
        <div class="row block-large-1-2 block-900-full work-positions">
            <div class="column" data-aos="fade-up">
                <div class="position">
                    <div class="position__header">
                        <h6>
                            <span class="position__co">99Designs</span>
                            <span class="position__pos">Designer (Freelancer)</span>
                        </h6>
                        <div class="position__timeframe">
                            2014 - 2017
                        </div>
                    </div>

                    <p>
                        Successfully created and implemented digital graphic design projects from concept through completion.
                        Worked closely with the client to create a vision, conceive designs, and consistently meet deadlines and requirements.
                    </p>
                </div>
            </div> <!-- end column -->
            <div class="column" data-aos="fade-up">
                <div class="position">
                    <div class="position__header">
                        <h6>
                            <span class="position__co">Dedeman</span>
                            <span class="position__pos">Sale Consultant</span>
                        </h6>
                        <div class="position__timeframe">
                            2017 - 2019
                        </div>
                    </div>

                    <p>
                        Met with key personnel to implement on-going sales planning.
                        Design of bathrooms and kitchens in the ViSoft3D program. Managed the day to day clientele business to meet marketing objectives,
                        and to help set measurable metrics for success.
                    </p>
                </div>
            </div> <!-- end column -->
        </div>
        <!-- CV
        ================================================== -->
        <div class="s-cta">

            <div class="row">
                <div class="column large-full">
                    <h2 class="section-desc" data-aos="fade-up">
                        Finde more in my CV!
                    </h2>
                </div>
            </div>

            <div class="row cta-content" data-aos="fade-up">
                <div class="column large-full">
                    <p>
                        You can read more about my biography, experience, skills, education and much more in the PDF attached bellow:
                    </p>
                    <a href="#" class="btn btn--primary h-full-width">Downloads My CV</a>
                </div>
            </div> <!-- end CV-content -->

        </div> <!-- end s-CV -->
    </div>

</section> <!-- end s-about -->
